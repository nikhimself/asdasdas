        ["dropcash"] = {
            ["ListName"] = "dropcash {amount}",
            ["Description"] = "(DA HOOD ONLY) - loops drop cash until you have dropped that amount",
            ["Aliases"] = {'dropcash'},
            ["Function"] = function(args, speaker)
                if tonumber(args[1]) ~= nil then
                    if tonumber(args[1]) >= 100 then
                        dropping = true
                        local DropTotal = tonumber(args[1])
                        local player = game.Players.LocalPlayer
                        local MainEvent = game.ReplicatedStorage.MainEvent
                        local StartCash = player.DataFolder.Currency.Value
                        local CurrentCash = player.DataFolder.Currency.Value
                        local function dropcash()
                            wait()
                            CurrentCash = player.DataFolder.Currency.Value
                            local MainScreenGui = player.PlayerGui.MainScreenGui
                            local TextBox = MainScreenGui.WalletDrop.TextBox
                            if DropTotal < 10000 then
                               TextBox.Text = tostring(DropTotal)
                            else
                              TextBox.Text = tostring(10000)
                            end
                            MainEvent:FireServer("DropMoney", MainScreenGui.WalletDrop.TextBox.Text)
                        end
                        repeat dropcash() until CurrentCash <= (StartCash - DropTotal) or dropping == false
                        dropping = false
                    end
                end
            end
        },
