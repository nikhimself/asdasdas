        ["rpglock"] = {
            ["ListName"] = "rpglock {player}",
            ["Description"] = "(DA HOOD ONLY) - rpg lock",
            ["Aliases"] = {'rpglock'},
            ["Function"] = function(args, speaker)
                local function findPlayer(name)
                    for _, Player in ipairs(Players:GetPlayers()) do
                        if (string.lower(name) == string.sub(string.lower(Player.Name), 1, #name)) then
                            return Player;
                        end
                    end
                end
                local target = findPlayer(tostring(args[1]))
                if target then
                    rpglocking = true
                    local Distance = 3
                    local Delay = 120
                    local player = game.Players.LocalPlayer
                    local loopFunction = function()
                        local Launcher = game.Workspace.Ignored:FindFirstChild('Launcher')
                        local Enemy = game.Workspace:FindFirstChild(target.Name) or game.Workspace.Players:FindFirstChild(target.Name)
                        if Launcher and Enemy then
                            Launcher:WaitForChild('Swoosh'):Stop()
                            if Delay < 0 then
                                Launcher.Position = Vector3.new(Enemy.UpperTorso.Position.X, Enemy.UpperTorso.Position.Y + Distance, Enemy.UpperTorso.Position.Z)
                                Distance = Distance - 0.1
                            else
                                Delay = Delay - 1
                                Launcher.Position = Vector3.new(0,300,0)
                            end
                        else
                            Delay = 120
                            Distance = 3
                        end
                    end;
                    local Loop
                    local Start = function()
                        Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                    end;
                    local Pause = function()
                        Loop:Disconnect()
                    end;
                    Start()
                    repeat wait() until rpglocking == false
                    Pause()
                end
            end
        },
