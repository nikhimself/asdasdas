        ["ping"] = {
            ["ListName"] = "ping",
            ["Description"] = "(DA HOOD ONLY) - changes animations gamepass to ping",
            ["Aliases"] = {'ping'},
            ["Function"] = function(args, speaker)
                local function getping()
                    local Animations = game.Players.LocalPlayer:FindFirstChild('PlayerGui'):FindFirstChild('MainScreenGui'):FindFirstChild('AnimationPack')  
                    if Animations then
                        Animations.TextScaled = true
                        Animations.Text = game:GetService("Stats").Network.ServerStatsItem["Data Ping"]:GetValueString()
                    end
                end
                while wait() do
                    local success, err = pcall(getping)
                end
            end
        },
        ["qtp"] = {
            ["ListName"] = "qtp",
            ["Description"] = "(DA HOOD ONLY) - enabled q to tp to mouse cursor",
            ["Aliases"] = {'antislow'},
            ["Function"] = function(args, speaker)
                qtp = true
                local player = game.Players.LocalPlayer
                local mouse = player:GetMouse()
                game:GetService('UserInputService').InputBegan:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.Q and not b then
                        if qtp == true then
                            local HUM = game:GetService("Workspace").Players:WaitForChild(player.Name).HumanoidRootPart
                            local OrientationX = HUM.Orientation.X
                            local OrientationY = HUM.Orientation.Y
                            local OrientationZ = HUM.Orientation.Z
                            local LeftHand = game:GetService("Workspace").Players:WaitForChild(player.Name).LeftHand
                            local RightHand = game:GetService("Workspace").Players:WaitForChild(player.Name).RightHand
                            if mouse.Target then 
                                local part = mouse.Target
                                local partSize = part.Size
                                local halfSize = partSize.Y/2
                                HUM.CFrame = CFrame.new(mouse.Hit.X,part.Position.Y + halfSize + 3,mouse.Hit.Z) * CFrame.Angles(math.rad(OrientationX),math.rad(OrientationY),math.rad(OrientationZ))
                                if not player.Character.LeftHand:FindFirstChild('LeftWrist') then
                                    LeftHand.Position = HUM.Parent.LeftLowerArm.Position
                                    RightHand.Position = HUM.Parent.RightLowerArm.Position
                                end
                            end
                        end
                    end
                end)
            end
        },
        ["unqtp"] = {
            ["ListName"] = "unqtp",
            ["Description"] = "(DA HOOD ONLY) - disabled q to tp to mouse cursor",
            ["Aliases"] = {'unqtp'},
            ["Function"] = function(args, speaker)
                qtp = false
            end
        },
        ["autoblock"] = {
            ["ListName"] = "autoblock {maxdistance}",
            ["Description"] = "(DA HOOD ONLY) - autoblock arg 1 default = 10 studs",
            ["Aliases"] = {'autoblock'},
            ["Function"] = function(args, speaker)
                autoblocking = true
                local MaxDistance = 10
                if tonumber(args[1]) ~= nil then
                    MaxDistance = tonumber(args[1])
                else
                    MaxDistance = 10
                end
                local forbidden = {
                    '[RPG]',
                    '[SMG]',
                    '[TacticalShotgun]',
                    '[AK47]',
                    '[AUG]',
                    '[Glock]',
                    '[Shotgun]',
                    '[Flamethrower]',
                    '[Silencer]',
                    '[AR]',
                    '[Revolver]',
                    '[SilencerAR]',
                    '[LMG]',
                    '[P90]',
                    '[DrumGun]',
                    '[Double-Barrel SG]',
                    '[Hamburger]',
                    '[Chicken]',
                    '[Pizza]',
                    '[Cranberry]',
                    '[Donut]',
                    '[Taco]',
                    '[Starblox Latte]',
                    '[BrownBag]',
                    '[Weights]',
                    '[HeavyWeights]',
                }
                local HoldingF = false
                local UIS = game:GetService('UserInputService')
                UIS.InputBegan:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.F and not b then
                        HoldingF = true
                        MainEvent:FireServer('Block', player.Name)
                    end
                end)
                UIS.InputEnded:Connect(function(key, b)
                    if key.KeyCode == Enum.KeyCode.F and not b then
                        HoldingF = false
                        if player.Character.BodyEffects:FindFirstChild('Block') then
                            player.Character.BodyEffects.Block:Destroy()
                        end
                    end
                end)
                local player = game.Players.LocalPlayer
                local MainEvent = game.ReplicatedStorage.MainEvent
                local Loop
                local loopFunction = function()
                    local success, problem = pcall(function()
                        local Found = false
                        for i,v in pairs(game.Workspace.Players:GetChildren()) do
                            local mag = (v.UpperTorso.Position - player.Character.HumanoidRootPart.Position).Magnitude
                            if mag <= MaxDistance then
                                if v.BodyEffects.Attacking.Value == true and not table.find(forbidden, v:FindFirstChildWhichIsA('Tool').Name ) and v.Name ~= player.Name then
                                    Found = true
                                    MainEvent:FireServer('Block', player.Name)
                                end
                            end
                        end
                        if Found == false and HoldingF == false then
                            if player.Character.BodyEffects:FindFirstChild('Block') then
                                player.Character.BodyEffects.Block:Destroy()
                            end
                        end
                    end)
                end;
                local Start = function()
                    Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                end;
                local Pause = function()
                    Loop:Disconnect()
                end;
                Start()
                repeat wait() until autoblocking == false
                Pause()

            end
        },
