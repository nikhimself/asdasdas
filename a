            ["Aliases"] = {'fashdrop'},
            ["Function"] = function(args, speaker)
                local player = game.Players.LocalPlayer
                local MainEvent = game.ReplicatedStorage.MainEvent
                fastdropping = true
                local CashDrop = 10000
                local function checkcash()
                    local cash = tonumber(player.DataFolder.Currency.Value)
                    if cash >= 10000 then
                        CashDrop = 10000
                    else
                        CashDrop = cash
                    end
                end
                local Loop
                local loopFunction = function()
                    local success, problem = pcall(function()
                        wait(.3)
                        checkcash()
                        local MainScreenGui = player.PlayerGui.MainScreenGui
                        local TextBox = MainScreenGui.WalletDrop.TextBox
                        TextBox.Text = CashDrop
                        MainEvent:FireServer("DropMoney", MainScreenGui.WalletDrop.TextBox.Text)
                    end)
                end;
                local Start = function()
                    Loop = game:GetService("RunService").Heartbeat:Connect(loopFunction);
                end;
                local Pause = function()
                    Loop:Disconnect()
                end;
                Start()
                repeat wait() until fastdropping == false
                Pause()
            end
