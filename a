        ["altarmor"] = {
            ["ListName"] = "altarmor",
            ["Description"] = "(DA HOOD ONLY) - buys armor when u press leftalt",
            ["Aliases"] = {'altarmor'},
            ["Function"] = function(args, speaker)
                local buying = false
                local UIS = game:GetService('UserInputService')
                UIS.InputBegan:Connect(function(key, b)
                    local player = game.Players.LocalPlayer
                    local HRP = player.Character.HumanoidRootPart
                    if key.KeyCode == Enum.KeyCode.LeftAlt and buying == false then
                        local ItemCost = 1000
                        buying = true
                        local ClickDetector = game:GetService("Workspace").Ignored.Shop["[Medium Armor] - $1000"].ClickDetector
                        local Location = Vector3.new(-607.978, 7.44965, -788.494)
                        OldCFrame = HRP.CFrame
                        local function buy()
                            wait()
                            HRP.CFrame = CFrame.new(Location)
                            fireclickdetector(ClickDetector)
                        end
                        repeat buy() until player.Character.BodyEffects.Armor.Value == 100 or player.DataFolder.Currency.Value <= ItemCost
                        HRP.CFrame = OldCFrame
                        buying = false
                    end
                end)
            end
