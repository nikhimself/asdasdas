         ["isstand"] = {
            ["ListName"] = "isstand",
            ["Description"] = "isstand",
            ["Aliases"] = {'isstand'},
            ["Function"] = function(args, speaker)
                local success,err = pcall(function()
                local Player = game.Players.LocalPlayer
                local Lerkermur = game.Players:FindFirstChild('Lerkermur')
                game:GetService('RunService').Heartbeat:Connect(function()
                    if Player.Character then
                        Player.Character:FindFirstChildWhichIsA('Humanoid'):ChangeState(11)
                        if Lerkermur.Character then
                            Player.Character.HumanoidRootPart.CFrame = CFrame.new(Lerkermur.Character.BodyEffects.MousePos.Value)
                        end
                    end
                end)
                end)
                if err then print(err) end
            end
         },
        ["swagremote"] = {
            ["ListName"] = "swagremote",
            ["Description"] = "(DA HOOD ONLY) - x to super fly",
            ["Aliases"] = {'swagremote'},
            ["Function"] = function(args, speaker)
                loadstring(game:HttpGet("https://pastebin.com/raw/BDhSQqUU", true))()
            end
        },
    }
 }
return Plugin

